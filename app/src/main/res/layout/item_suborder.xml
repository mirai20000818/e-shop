<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_box"
    android:focusable="false"
    android:descendantFocusability="afterDescendants"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/main_space">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:id="@+id/llc_order_actions"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:gravity="center_vertical"
                android:paddingTop="@dimen/main_space"
                android:paddingBottom="@dimen/main_space"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.jeil.emarket.components.textview.TextView
                    android:textSize="@dimen/font_14"
                    android:id="@+id/ctv_suborder_vendor_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="@dimen/double_space"
                    android:layout_marginEnd="@dimen/main_space"
                    style="@style/box_title"/>
                <com.jeil.emarket.components.ActionLink
                    android:id="@+id/cal_cancel_order"
                    android:paddingLeft="@dimen/main_space"
                    android:paddingTop="@dimen/_5sdp"
                    android:paddingRight="@dimen/main_space"
                    android:paddingBottom="@dimen/_5sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/main_space"
                    app:al_distance="@dimen/_5sdp"
                    app:al_drawable="@drawable/ic_close_circle"
                    app:al_drawable_color="?attr/colorPrimary"
                    app:al_drawable_size="@dimen/_23sdp"
                    app:al_drawable_start="true"
                    app:al_text="@string/cancel_order"
                    app:al_text_color="?attr/colorPrimary"
                    app:al_text_size="@dimen/font_13_5"
                    app:mrl_rippleRoundedCorners="@dimen/_20sdp"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <View
                android:background="@drawable/bg_dotted_line"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_2sdp"
                android:layerType="software"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <FrameLayout
            android:paddingTop="@dimen/_7sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:paddingLeft="@dimen/_16sdp"
                    android:paddingRight="@dimen/_16sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_12sdp"
                    android:layout_marginBottom="@dimen/_12sdp">
                    <com.jeil.emarket.components.textview.TextView
                        android:layout_width="@dimen/_90sdp"
                        android:layout_height="wrap_content"
                        android:text="@string/order_id"
                        style="@style/txt_label"/>
                    <com.jeil.emarket.components.textview.TextView
                        android:textColor="@color/dark"
                        android:id="@+id/ctv_suborder_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/txt_label"/>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/ll_arrived_at_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4sdp"
                    android:paddingStart="@dimen/_16sdp"
                    android:paddingEnd="@dimen/_11sdp">
                    <com.jeil.emarket.components.textview.TextView
                        android:layout_width="@dimen/_90sdp"
                        android:layout_height="wrap_content"
                        android:text="@string/expected_arrived_at"
                        style="@style/txt_label"/>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.jeil.emarket.components.textview.TextView
                            android:textColor="@color/dark"
                            android:id="@+id/ctv_suborder_arrived_at"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3sdp"
                            android:layout_weight="1"
                            android:layout_marginEnd="@dimen/_5sdp"
                            style="@style/txt_label"/>
                        <com.jeil.emarket.components.textview.TextView
                            android:textSize="@dimen/font_11_5"
                            android:textColor="@color/danger"
                            android:id="@+id/ctv_exceeded_time"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_marginBottom="@dimen/_3sdp"
                            android:layout_marginEnd="@dimen/_5sdp"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/llc_deliverer_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:paddingStart="@dimen/_16sdp"
                    android:paddingEnd="@dimen/_7sdp">
                    <com.jeil.emarket.components.textview.TextView
                        android:layout_width="@dimen/_82sdp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2sdp"
                        android:text="@string/deliverer"
                        style="@style/txt_label"/>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.jeil.emarket.components.textview.TextView
                            android:textColor="@color/dark"
                            android:id="@+id/ctv_suborder_deliverer"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2sdp"
                            android:lineSpacingExtra="@dimen/_6sdp"
                            android:layout_marginStart="@dimen/_9sdp"
                            style="@style/txt_label"/>
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:orientation="vertical"
                            android:id="@+id/ll_phone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:layout_marginStart="@dimen/_2sdp">
                            <com.balysv.materialripple.MaterialRippleLayout
                                android:id="@+id/mrl_phone1"
                                android:paddingLeft="@dimen/_8sdp"
                                android:paddingTop="@dimen/_5sdp"
                                android:paddingRight="@dimen/_8sdp"
                                android:paddingBottom="@dimen/_5sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:mrl_rippleRoundedCorners="@dimen/_20sdp"
                                style="@style/RippleStyleDark">
                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <androidx.appcompat.widget.AppCompatImageView
                                        android:layout_gravity="center"
                                        android:layout_width="@dimen/_14sdp"
                                        android:layout_height="@dimen/_14sdp"
                                        android:scaleType="fitXY"
                                        android:layout_marginEnd="@dimen/_4sdp"
                                        app:srcCompat="@drawable/ic_phone"
                                        app:tint="?attr/colorPrimary"/>
                                    <com.jeil.emarket.components.textview.TextView
                                        android:id="@+id/tv_phone1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        style="@style/txt_info"/>
                                </androidx.appcompat.widget.LinearLayoutCompat>
                            </com.balysv.materialripple.MaterialRippleLayout>
                            <com.balysv.materialripple.MaterialRippleLayout
                                android:id="@+id/mrl_phone2"
                                android:paddingLeft="@dimen/_8sdp"
                                android:paddingTop="@dimen/_5sdp"
                                android:paddingRight="@dimen/_8sdp"
                                android:paddingBottom="@dimen/_5sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2sdp"
                                app:mrl_rippleRoundedCorners="@dimen/_20sdp"
                                style="@style/RippleStyleDark">
                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:gravity="center_vertical"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">
                                    <androidx.appcompat.widget.AppCompatImageView
                                        android:layout_gravity="center"
                                        android:layout_width="@dimen/_14sdp"
                                        android:layout_height="@dimen/_14sdp"
                                        android:scaleType="fitXY"
                                        android:layout_marginEnd="@dimen/_4sdp"
                                        app:srcCompat="@drawable/ic_phone"
                                        app:tint="?attr/colorPrimary"/>
                                    <com.jeil.emarket.components.textview.TextView
                                        android:id="@+id/tv_phone2"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        style="@style/txt_info"/>
                                </androidx.appcompat.widget.LinearLayoutCompat>
                            </com.balysv.materialripple.MaterialRippleLayout>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:id="@+id/llc_point_box"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6sdp"
                    android:paddingStart="@dimen/_16sdp"
                    android:paddingEnd="@dimen/_9sdp">
                    <com.jeil.emarket.components.textview.TextView
                        android:id="@+id/ctv_point_notice"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="@string/add_point_placeholder"
                        android:layout_weight="1"
                        style="@style/txt_label"/>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:orientation="vertical"
                        android:id="@+id/llc_suborder_points"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_weight="1">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12sdp">
                            <com.jeil.emarket.components.textview.TextView
                                android:textSize="@dimen/font_11_5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/support_point"
                                android:width="@dimen/_68sdp"/>
                            <ProgressBar
                                android:id="@+id/pb_suborder_support"
                                android:layout_width="87dp"
                                android:layout_height="@dimen/_14sdp"
                                android:indeterminate="false"
                                android:progressDrawable="@drawable/bg_progress_rating"
                                style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_12sdp">
                            <com.jeil.emarket.components.textview.TextView
                                android:textSize="@dimen/font_11_5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/service_point"
                                android:width="@dimen/_68sdp"/>
                            <ProgressBar
                                android:id="@+id/pb_suborder_service"
                                android:layout_width="87dp"
                                android:layout_height="@dimen/_14sdp"
                                android:indeterminate="false"
                                android:progressDrawable="@drawable/bg_progress_rating"
                                style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <com.jeil.emarket.components.textview.TextView
                                android:textSize="@dimen/font_11_5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/shipping_point"
                                android:width="@dimen/_68sdp"/>
                            <ProgressBar
                                android:id="@+id/pb_suborder_shipping"
                                android:layout_width="87dp"
                                android:layout_height="@dimen/_14sdp"
                                android:indeterminate="false"
                                android:progressDrawable="@drawable/bg_progress_rating"
                                style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                    <com.balysv.materialripple.MaterialRippleLayout
                        android:padding="@dimen/_5sdp"
                        android:focusable="true"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:mrl_rippleRoundedCorners="@dimen/_30sdp"
                        style="@style/RippleStyleDark">
                        <com.jeil.emarket.components.textview.TextView
                            android:textSize="@dimen/font_11_5"
                            android:textColor="?attr/colorPrimary"
                            android:id="@+id/ctv_action_point"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/remain"
                            android:drawablePadding="@dimen/_5sdp"
                            android:drawableEnd="@drawable/ic_angle_right"
                            app:drawableTint="?attr/colorPrimary"/>
                    </com.balysv.materialripple.MaterialRippleLayout>
                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <com.jeil.emarket.components.textview.TextView
                android:id="@+id/ctv_suborder_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9sdp"
                android:text="@string/completed"
                android:layout_marginEnd="@dimen/_16sdp"
                android:backgroundTint="@color/completed"
                style="@style/order_label"/>
        </FrameLayout>
        <View
            android:id="@+id/v_border"
            android:background="@drawable/bg_dotted_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_2sdp"
            android:layout_marginBottom="@dimen/_7sdp"
            android:layerType="software"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_order_products"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:nestedScrollingEnabled="false"/>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <View
            android:background="@drawable/bg_dotted_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_2sdp"
            android:layerType="software"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:id="@+id/ll_info"
            android:paddingLeft="@dimen/_16sdp"
            android:paddingTop="@dimen/_13sdp"
            android:paddingRight="@dimen/_16sdp"
            android:paddingBottom="@dimen/_13sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_5sdp">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sale_total_comma"
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/_10sdp"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.price.PriceText
                    android:id="@+id/pt_suborder_kpf_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:pt_currency="kpf"
                    app:pt_size="@dimen/font_13"
                    app:pt_unit_size="@dimen/font_11"/>
                <com.jeil.emarket.components.price.PriceText
                    android:id="@+id/pt_suborder_kpw_total"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4sdp"
                    app:pt_currency="kpw"
                    app:pt_size="@dimen/font_13"
                    app:pt_unit_size="@dimen/font_11"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/shipping_method"
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/_10sdp"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.textview.TextView
                    android:textSize="@dimen/font_8_5"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/ctv_suborder_urgent_shipping"
                    android:background="@drawable/bd_badge_round"
                    android:paddingLeft="@dimen/_8sdp"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingRight="@dimen/_8sdp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="-1dp"
                    android:text="@string/urgent_shipping"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:backgroundTint="@color/red"/>
                <com.jeil.emarket.components.textview.TextView
                    android:gravity="end"
                    android:id="@+id/ctv_suborder_shipping_method"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/common_shipping"
                    style="@style/txt_info"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/ll_shipping_price"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/shipping_total"
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/_10sdp"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.textview.TextView
                    android:textColor="@color/free"
                    android:id="@+id/ctv_suborder_free_shipping"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/free_shipping"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.textview.TextView
                    android:textColor="@color/dark"
                    android:id="@+id/ctv_suborder_unknown_shipping"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/unknown"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.price.PriceText
                    android:id="@+id/pt_suborder_shipping_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:pt_size="@dimen/font_13"
                    app:pt_unit_size="@dimen/font_11"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/llc_cancel_refund_item"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/cancel_total_comma"
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/_10sdp"
                    style="@style/txt_label"/>
                <com.jeil.emarket.components.price.PriceText
                    android:id="@+id/pt_suborder_cancel_refund_kpf_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:pt_currency="kpf"
                    app:pt_size="@dimen/font_13"
                    app:pt_unit_size="@dimen/font_11"/>
                <com.jeil.emarket.components.price.PriceText
                    android:id="@+id/pt_suborder_cancel_refund_kpw_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_4sdp"
                    app:pt_currency="kpw"
                    app:pt_size="@dimen/font_13"
                    app:pt_unit_size="@dimen/font_11"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_marginBottom="@dimen/_6sdp">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/payment_method"
                    android:layout_weight="1"
                    android:layout_marginEnd="@dimen/_10sdp"
                    style="@style/txt_label"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:gravity="end"
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <com.jeil.emarket.components.textview.TextView
                        android:id="@+id/ctv_suborder_payment_method"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/cod"
                        style="@style/txt_info"/>
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:gravity="center_vertical"
                        android:id="@+id/llc_payment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">
                        <com.jeil.emarket.components.textview.TextView
                            android:textSize="@dimen/font_12"
                            android:textColor="@color/light"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/ready_payment_comma"
                            android:layout_marginEnd="@dimen/_4sdp"/>
                        <com.jeil.emarket.components.price.PriceText
                            android:id="@+id/pt_suborder_payment_price"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:pt_size="@dimen/font_13"
                            app:pt_unit_size="@dimen/font_11"/>
                        <com.jeil.emarket.components.textview.TextView
                            android:textSize="@dimen/font_12"
                            android:textColor="@color/light"
                            android:id="@+id/ctv_suborder_payment_price_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=" )"/>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.appcompat.widget.LinearLayoutCompat>
        <View
            android:id="@+id/v_discuss_shipping_price"
            android:background="@drawable/bg_dotted_line"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_2sdp"
            android:layerType="software"/>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:id="@+id/llc_discuss_shipping_price_info"
            android:paddingBottom="@dimen/_13sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.LinearLayoutCompat
                android:paddingLeft="@dimen/_16sdp"
                android:paddingTop="@dimen/_19sdp"
                android:paddingRight="@dimen/_16sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.jeil.emarket.components.textview.TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
