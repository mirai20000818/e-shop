                                            app:pt_size="@dimen/font_12"
                                            app:pt_unit_size="@dimen/font_11_5"/>
                                    </androidx.appcompat.widget.LinearLayoutCompat>
                                </androidx.appcompat.widget.LinearLayoutCompat>
                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_9sdp">
                                    <com.jeil.emarket.components.textview.TextView
                                        android:textSize="@dimen/font_11_5"
                                        android:textColor="@color/transparent"
                                        android:id="@+id/tv_completed_order_label"
                                        android:background="@drawable/bg_grey_round"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/order"
                                        android:layout_marginEnd="@dimen/_6sdp"/>
                                    <com.jeil.emarket.components.textview.TextView
                                        android:textSize="@dimen/font_11_5"
                                        android:textColor="@color/transparent"
                                        android:id="@+id/tv_completed_order_count"
                                        android:background="@drawable/bg_grey_round"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/shimmer_price_approved_no_comma"
