                        android:padding="@dimen/_6sdp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_18sdp"
                        android:layout_marginStart="@dimen/_70sdp"
                        app:mrl_rippleRoundedCorners="@dimen/_20sdp"
                        style="@style/RippleStyleDark">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <androidx.appcompat.widget.AppCompatImageView
                                android:theme="@style/Theme.AppCompat"
                                android:id="@+id/iv_request_shimmer"
                                android:background="@drawable/bd_grey_ellipse"
                                android:layout_width="@dimen/_13sdp"
                                android:layout_height="@dimen/_13sdp"
                                android:tint="@color/white"
                                app:srcCompat="@drawable/ic_danger_circle"/>
                            <androidx.appcompat.widget.AppCompatImageView
                                android:theme="@style/Theme.AppCompat"
                                android:id="@+id/iv_request"
                                android:visibility="gone"
                                android:layout_width="@dimen/_13sdp"
                                android:layout_height="@dimen/_13sdp"
                                android:tint="@color/body"
                                app:srcCompat="@drawable/ic_danger_circle"/>
                        </androidx.appcompat.widget.LinearLayoutCompat>
                    </com.balysv.materialripple.MaterialRippleLayout>
