<?xml version="1.0" encoding="utf-8"?>
<com.jeil.emarket.components.swiperefresh.WaveSwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/wsr_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:nestedScrollingEnabled="false">
    <androidx.core.widget.NestedScrollView
        android:id="@+id/nsv_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        android:overScrollMode="never">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <include
                android:id="@+id/header"
                layout="@layout/layout_top_bar_vendor"/>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/main_space">
                <include
                    android:id="@+id/ll_section_order_chart"
                    layout="@layout/layout_dashboard_order_chart"/>
                <include
                    android:id="@+id/ll_section_sale_chart"
                    layout="@layout/layout_dashboard_chart"/>
                <include
                    android:id="@+id/llc_section_overview"
                    layout="@layout/layout_dashboard_overview"/>
                <include layout="@layout/layout_dashboard_ticket"/>
            </androidx.appcompat.widget.LinearLayoutCompat>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.core.widget.NestedScrollView>
</com.jeil.emarket.components.swiperefresh.WaveSwipeRefreshLayout>
